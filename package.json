{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "buildNpmdoc": "xhr",
    "description": "#### basic api documentation for  [xhr (v2.4.0)](https://github.com/naugtur/xhr)  [![npm package](https://img.shields.io/npm/v/npmdoc-xhr.svg?style=flat-square)](https://www.npmjs.org/package/npmdoc-xhr) [![travis-ci.org build-status](https://api.travis-ci.org/npmdoc/node-npmdoc-xhr.svg)](https://travis-ci.org/npmdoc/node-npmdoc-xhr)",
    "devDependencies": {
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "utility2": "kaizhu256/node-utility2#alpha",
        "xhr": "*"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmdoc/node-npmdoc-xhr",
    "keywords": [
        "documentation",
        "xhr"
    ],
    "license": "MIT",
    "main": "lib.npmdoc_xhr.js",
    "name": "npmdoc-xhr",
    "nameAlias": "npmdoc_xhr",
    "nameOriginal": "npmdoc-xhr",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-xhr.git"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "(set -e; npm install \"kaizhu256/node-utility2#alpha\"; utility2 shDeployHeroku)",
        "postinstall": "if [ -f npm_scripts.sh ]; then ./npm_scripts.sh postinstall; fi",
        "start": "(set -e; export PORT=${PORT:-8080}; utility2 start test.js)",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "2017.4.27"
}
